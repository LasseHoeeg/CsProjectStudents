        <ListBox x:Name="ListeKurser"   Grid.Column="1" Margin="10,38,10,58" Grid.Row="1"/>
        <Button x:Name="VisAlleStuderende" Grid.Column="1" Content="Vis alle studerende" HorizontalAlignment="Left" Margin="12,169,0,0" VerticalAlignment="Top" Height="37" Width="113" Click="VisAlleStuderende_Click" />
        <Button x:Name="Tilfoej_Copy1" Grid.Column="1" Content="Vis alle kurser" HorizontalAlignment="Left" Margin="12,170,0,0" Grid.Row="1" VerticalAlignment="Top" Height="37" Width="113" />
        <Label Grid.Column="1" Content="Alle Kurser" HorizontalAlignment="Left" Margin="12,7,0,0" VerticalAlignment="Top" Grid.Row="1"/>


    </Grid>
</Window>
